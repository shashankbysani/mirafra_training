#include <iostream>
using namespace std;

int main() {
    
    int matrix[3][3]; // Declaration of a 2D array
    cout << "Enter 9 elements for the 3x3 matrix:" << endl;
    for (int i = 0; i < 3; i++) {        
        for (int j = 0; j < 3; j++) {    
            cin >> matrix[i][j];         
       }
    }
    cout << "\nMatrix elements are:" << endl;
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            cout << matrix[i][j] << " "; 
       }
        cout << endl;
    }
    cout << "\nChanging matrix[1][1] ." << endl;
    matrix[1][1] = 99; // Updating the value at position (1, 1)
    cout << "Updated matrix is:" << endl;
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            cout << matrix[i][j] << " "; 
       }
        cout << endl;
   }

    return 0;
}
